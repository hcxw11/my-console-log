{
  "name": "my-log",
  "displayName": "my-log",
  "description": "create log",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.30.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:myConsoleLog.displayLog",
    "onCommand:myConsoleLog.deleteAllLog",
    "onCommand:myConsoleLog.commentAllLog",
    "onCommand:myConsoleLog.unCommentAllLog"
  ],
  "main": "./out/extension",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "My Console Log Configuration",
      "properties": {
        "normalLogFormat": {
          "type": "string",
          "default": "'{C} -> {F} -> {V}:', {V}",
          "description": "normal log format."
        },
        "noVarLogFormat": {
          "type": "string",
          "default": "'{C} -> {F}'",
          "description": "no variable log format."
        }
      }
    },
    "commands": [
      {
        "command": "myConsoleLog.displayLog",
        "title": "my console log: add log"
      },
      {
        "command": "myConsoleLog.deleteAllLog",
        "title": "my console log: delete all log"
      },
      {
        "command": "myConsoleLog.commentAllLog",
        "title": "my console log: comment all log"
      },
      {
        "command": "myConsoleLog.unCommentAllLog",
        "title": "my console log: unComment all log"
      }
    ],
    "keybindings": [
      {
        "command": "myConsoleLog.displayLog",
        "key": "alt+shift+l",
        "mac": "alt+shift+l"
      },
      {
        "command": "myConsoleLog.deleteAllLog",
        "key": "alt+shift+d",
        "mac": "alt+shift+d"
      },
      {
        "command": "myConsoleLog.commentAllLog",
        "key": "alt+shift+c",
        "mac": "alt+shift+c"
      },
      {
        "command": "myConsoleLog.unCommentAllLog",
        "key": "alt+shift+u",
        "mac": "alt+shift+u"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.1.4",
    "vscode": "^1.1.25",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "ramda": "^0.26.1"
  }
}
